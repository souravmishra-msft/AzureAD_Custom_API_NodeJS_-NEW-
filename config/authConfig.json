{
    "credentials": {
        "tenantID": "{tenant-id}",
        "clientID": "{app-id}",
        "audience": "{app-id}"
    },
    "resource": {
        "scope": ["items.read", "items.write", "items.read.all"],
    },
    "metadata": {
        "authority": "login.microsoftonline.com",
        "discovery": ".well-known/openid-configuration",
        "keys": "discovery/v2.0/keys",
